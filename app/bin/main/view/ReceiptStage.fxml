<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TicketBooking.Actions.RegistrationAlertStage">
    <children>
        <Label fx:id="RegisterAlertLabel" layoutX="104.0" layoutY="91.0" textFill="RED" text="You need to log in to book movies">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Button fx:id="switchToDefaultStageButton" layoutX="237.0" layoutY="302.0" mnemonicParsing="false" onAction="#setSwitchToDefaultStage" prefHeight="35.0" prefWidth="124.0" text="Back">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="switchToRegisterButton" layoutX="237.0" layoutY="344.0" mnemonicParsing="false" onAction="#switchToRegister" prefHeight="35.0" prefWidth="124.0" text="Register Now">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Label fx:id="errorLabel" alignment="CENTER" layoutY="216.0" prefHeight="19.0" prefWidth="600.0" textFill="RED">
            <font>
                <Font size="13.0" />
            </font>
        </Label>

    </children>
</AnchorPane>
