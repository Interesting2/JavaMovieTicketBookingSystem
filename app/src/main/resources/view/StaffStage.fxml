<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="500.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ATM_App.Atm.Actions.LoginStage">
    <children>
        <Button fx:id="MovieManageButton" layoutX="100.0" layoutY="150.0" mnemonicParsing="false" onAction="#login" prefHeight="35.0" prefWidth="150.0" text="Movie Manage">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="GiftCardManageButton" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#switchToRegisterStage" prefHeight="35.0" prefWidth="150.0" text="Gift Card Manage">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
        <Button fx:id="reportButton" layoutX="100.0" layoutY="250.0" mnemonicParsing="false" onAction="#switchToGuestStage" prefHeight="35.0" prefWidth="150.0" text="Get Report">
            <font>
                <Font size="15.0" />
            </font>
        </Button>

        <Button fx:id="BackButton" layoutX="100.0" layoutY="300.0" mnemonicParsing="false" onAction="#switchToGuestStage" prefHeight="35.0" prefWidth="150.0" text="Back">
            <font>
                <Font size="15.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
